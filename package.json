{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ashercast",
  "title": "AsherCast",
  "description": "Search My Media",
  "icon": "command-icon.png",
  "author": "kysung95",
  "license": "MIT",
  "commands": [
    {
      "name": "search-medias",
      "title": "Search Medias",
      "subtitle": "Asher",
      "description": "",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.1",
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "preferences": [
    {
      "name": "wistiaApiToken",
      "title": "API Token",
      "description": "API Token",
      "type": "password",
      "default": "",
      "required": true
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
